<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cz.muni.fi.fits.gui.view.controllers.OperationTabsViewController">
    <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="0.0"
             AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    <Separator layoutX="14.0" layoutY="348.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="10.0"
               AnchorPane.rightAnchor="10.0"/>
    <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefHeight="49.0" spacing="20.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="25.0"
                     progress="0.0" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets left="20.0"/>
            </HBox.margin>
        </ProgressBar>
        <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true"
                maxHeight="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#onStartEditor"
                prefHeight="35.0" text="%editor.start">
            <HBox.margin>
                <Insets right="20.0"/>
            </HBox.margin>
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
    </HBox>
</AnchorPane>
